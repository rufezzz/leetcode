#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
#include <unordered_map>
#include <fstream>


class Solution {
public:
    int titleToNumber(std::string columnTitle) {
        int n=columnTitle.size();
        int j=n-1;
        int ans=0;
        int d;
        for(int i=0;i<n;i++)
        {
            d=columnTitle[i]-'A'+1;
            ans=ans+(d*pow(26,j));
            j--;
        }   
         return ans;
    }
};

int main()
{
   std::string s = "ZY";

    Solution *solution = new Solution();
    solution->titleToNumber(s);

    delete solution;

    return 0;
}
