
#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
#include <unordered_map>
#include <fstream>


class Solution {
public:
    char findTheDifference(std::string s, std::string t) {
        sort(s.begin(),s.end());
        sort(t.begin(),t.end());
        for(int i=0;i<t.size();i++){
            if(i>=s.size()){
                char t1 = t[i];
                return t1;
            }
            if(t[i]!=s[i]){
                char t1 = t[i];
                return t1;
            }
        }
        return NULL;
    }
};

int main()
{
   std::string s = "abcd";
   std::string t = "abcde";

    Solution *solution = new Solution();
    solution->findTheDifference(s,t);

    delete solution;

    return 0;
}
