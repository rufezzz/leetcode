#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
#include <unordered_map>
#include <fstream>

class Solution {
public:
    int missingNumber(std::vector<int>& nums) {
        int n=nums.size();
        sort(nums.begin(),nums.end());
        for(int i=0;i<n;i++){
            if(nums[i]!=i) return i;
        }
    return n;
    }
};

int main()
{
    std::vector<int> nums = {3,0,1};

    Solution *solution = new Solution();
    solution->missingNumber(nums);

    delete solution;

    return 0;
}
